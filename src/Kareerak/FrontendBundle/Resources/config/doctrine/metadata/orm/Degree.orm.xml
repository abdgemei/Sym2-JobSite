<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Degree" table="degree">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="faculty" type="string" column="faculty" length="255"/>
    <field name="major" type="string" column="major" length="255"/>
    <field name="gradeEg" type="string" column="grade_eg" length="255"/>
    <field name="gradeGpa" type="string" column="grade_gpa" length="255"/>
    <field name="honours" type="string" column="honours" length="255"/>
    <many-to-one field="degreeLevel" target-entity="DegreeLevel" orphan-removal="">
      <join-columns>
        <join-column name="degree_level_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="country" target-entity="Country" orphan-removal="">
      <join-columns>
        <join-column name="country_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="city" target-entity="City" orphan-removal="">
      <join-columns>
        <join-column name="city_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="uni" target-entity="University" orphan-removal="">
      <join-columns>
        <join-column name="uni_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="seeker" target-entity="Seeker" orphan-removal="">
      <join-columns>
        <join-column name="seeker_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
