<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Order" table="order">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="timestamp" type="datetime" column="timestamp"/>
    <field name="status" type="string" column="status" length="255"/>
    <many-to-one field="employer" target-entity="Employer" orphan-removal="">
      <join-columns>
        <join-column name="employer_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="mtype" target-entity="MembershipType" orphan-removal="">
      <join-columns>
        <join-column name="mtype_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
